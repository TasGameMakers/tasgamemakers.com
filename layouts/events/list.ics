BEGIN:VCALENDAR{{ "\r\n" -}}
VERSION:2.0{{ "\r\n" -}}
PRODID:-//Tas Game Makers//Events//EN{{ "\r\n" -}}
CALSCALE:GREGORIAN{{ "\r\n" -}}
METHOD:PUBLISH{{ "\r\n" -}}
X-WR-CALNAME:Tas Game Makers Events{{ "\r\n" -}}
X-WR-TIMEZONE:Australia/Hobart{{ "\r\n" -}}
{{ range .Pages -}}
BEGIN:VEVENT{{ "\r\n" -}}
UID:{{ .Permalink }}{{ "\r\n" -}}
DTSTART:{{ .Date.UTC.Format "20060102T150405Z" }}{{ "\r\n" -}}
{{ with .Params.end_date -}}
DTEND:{{ (time .).UTC.Format "20060102T150405Z" }}{{ "\r\n" -}}
{{ end -}}
SUMMARY:Tas Game Makers: {{ .Title }}{{ "\r\n" -}}
DESCRIPTION:{{ .Params.summary | default .Title }}{{ "\r\n" -}}
{{ with .Params.location -}}
LOCATION:{{ . }}{{ "\r\n" -}}
{{ end -}}
URL:{{ .Permalink }}{{ "\r\n" -}}
END:VEVENT{{ "\r\n" -}}
{{ end -}}
END:VCALENDAR{{ "\r\n" -}}
